<% provide(:title, "部屋一覧") %>
<div class="container-fluid">
  <div class="row">
    <div class="col-12 index-header"></div>
    <div class="col-4">
    </div>
    <div class="col-4">
    <%= render 'rooms/form', room: Room.new %>
      <ul>
        <% @rooms.each do |room| %>
        <li><%= link_to " - #{room.name}", "/rooms/#{room.id}" %></li>
        <% end %>
      </ul>
    </div>
    <div class="col-4"></div>
  </div>
</div>